{
  "name": "interpol",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/interpol.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle --jsx React.createElement",
    "build:watch": "microbundle watch --no-sourcemap --format esm --jsx React.createElement",
    "dev:basic": "pnpm --filter ./examples/basic run dev",
    "dev:basic-timeline": "pnpm --filter ./examples/basic-timeline run dev",
    "dev:interport-repeat-refresh": "pnpm --filter ./examples/interpol-repeat-refresh run dev",
    "dev:interport-reverse-ease": "pnpm --filter ./examples/interpol-reverse-ease run dev",
    "test:watch": "vitest --reporter verbose",
    "test": "vitest run",
    "pre-publish": "npm run build && npm run test"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "vitest": "^0.23.4"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "dependencies": {
    "@wbe/debug": "^1.0.0",
    "vite": "^3.1.3"
  }
}
