{
  "name": "@wbe/interpol",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/interpol.git"
  },
  "keywords": [
    "interpol",
    "interpolation",
    "animation",
    "timeline"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && microbundle --jsx React.createElement",
    "build:watch": "microbundle watch --no-sourcemap --format esm --jsx React.createElement",
    "dev:interpol-basic": "pnpm --filter ./examples/interpol-basic run dev",
    "dev:timeline-basic": "pnpm --filter ./examples/timeline-basic run dev",
    "dev:idom-basic": "pnpm --filter ./examples/idom-basic run dev",
    "test:watch": "vitest --reporter verbose",
    "test": "vitest run",
    "pre-publish": "npm run build && npm run test"
  },
  "dependencies": {
    "@wbe/debug": "^1.1.0"
  },
  "devDependencies": {
    "jsdom": "^21.1.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  }
}
