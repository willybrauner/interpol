{
  "name": "psap",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/psap.git"
  },
  "keywords": [
    "psap",
    "interpol",
    "interpolation",
    "animation",
    "timeline",
    "dom"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run build:interpol && pnpm run build:psap",
    "build:interpol": "pnpm --filter ./packages/interpol run build",
    "build:interpol:watch": "pnpm --filter ./packages/interpol run build:watch",
    "build:psap": "pnpm --filter ./packages/psap run build",
    "build:psap:watch": "pnpm --filter ./packages/psap run build:watch",
    "dev:interpol": "pnpm --filter ./examples/interpol run dev",
    "dev:timeline": "pnpm --filter ./examples/timeline run dev",
    "dev:psap": "pnpm --filter ./examples/psap run dev",
    "test:watch": "vitest --reporter verbose",
    "test": "vitest run",
    "pre-publish": "npm run build && npm run test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^20.1.0",
    "concurrently": "^8.0.1",
    "jsdom": "^22.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  }
}
