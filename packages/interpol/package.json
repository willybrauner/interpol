{
  "name": "@wbe/interpol",
  "description": "Interpolates values with a GSAP-like API ~ 3kB",
  "author": {
    "name": "Willy Brauner",
    "url": "https://willybrauner.com"
  },
  "version": "0.20.2",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/interpol.js",
      "require": "./dist/interpol.cjs"
    }
  },
  "main": "./dist/interpol.cjs",
  "module": "./dist/interpol.js",
  "types": "./dist/interpol.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/interpol.git"
  },
  "keywords": [
    "interpol",
    "interpolation",
    "animation",
    "anim",
    "timeline",
    "motion"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:watch": "tsup --watch --sourcemap"
  },
  "devDependencies": {
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  }
}
