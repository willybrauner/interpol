{
  "name": "@wbe/interpol",
  "description": "Interpolates values with a GSAP-like API ~ 3kB",
  "author": {
    "name": "Willy Brauner",
    "url": "https://willybrauner.com"
  },
  "version": "0.26.0",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/interpol.js",
      "require": "./dist/interpol.cjs"
    }
  },
  "main": "./dist/interpol.cjs",
  "module": "./dist/interpol.js",
  "types": "./dist/interpol.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/interpol.git"
  },
  "keywords": [
    "interpol",
    "interpolation",
    "animation",
    "anim",
    "timeline",
    "motion"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:watch": "tsup --watch --sourcemap"
  },
  "devDependencies": {
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
